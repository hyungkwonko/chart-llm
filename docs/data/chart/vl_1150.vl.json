{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "A bubbleplot showing horsepower on x, miles per gallons on y, and binned acceleration on size.",
  "data": {
    "url": "https://raw.githubusercontent.com/dannysupple/dannysupple.github.io/main/test(pop1).csv"
  },
  "mark": "point",
  "encoding": {
    "x": {
      "field": "points",
      "type": "quantitative",
      "title": "Average points"
    },
    "y": {
      "field": "gdp",
      "type": "quantitative",
      "title": "GDP per capita"
    },
    "size": {
      "field": "pop",
      "type": "quantitative",
      "title": "Population"
    }
  },
  "config": {}
}
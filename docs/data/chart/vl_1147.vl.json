{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Google's stock price over time.",
  "data": {
    "url": "https://raw.githubusercontent.com/dannysupple/dannysupple.github.io/main/wiidglobal_12.csv"
  },
  "width": 340,
  "height": 340,
  "mark": "line",
  "encoding": {
    "x": {
      "field": "year",
      "type": "temporal",
      "title": "Year"
    },
    "y": {
      "field": "gini",
      "type": "quantitative",
      "scale": {
        "domain": [
          0,
          100
        ]
      },
      "title": "Gini coefficient"
    }
  },
  "config": {}
}
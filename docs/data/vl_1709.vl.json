{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "height": 300,
  "layer": [
    {
      "data": {
        "format": {
          "feature": "countries1",
          "type": "topojson"
        },
        "url": "https://gicentre.github.io/data/geoTutorials/world-110m.json"
      },
      "mark": {
        "fill": "rgb(149,181,146)",
        "type": "geoshape"
      }

{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "IMF lending Outstanding as a Percentage of Global GDP ",
    "subtitle": "Credit outstanding to all members (percentage of GDP), source:IMF and World Bank",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 9,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/Emilysquires02/Emilysquires02.github.io/main/data_IMF_creditOutstanding%20(1).csv"
  },
  "height": 300,
  "width": 300,
  "mark": {
    "type": "bar",
    "color": "#423abb"
  },
  "encoding": {
    "x": {
      "field": "Year",
      "type": "temporal",
      "axis": {
        "title": null,
        "grid": false,
        "ticks": false,
        "labelAngle": 45
      }
    },
    "y": {
      "field": "Percentage ",
      "type": "quantitative",
      "title": "Pecentage of Global GDP (in US$)",
      "axis": {
        "grid": false
      }
    }
  },
  "config": {}
}
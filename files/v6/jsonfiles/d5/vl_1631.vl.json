{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "config": {
    "font": "Comic Sans MS"
  },
  "data": {
    "url": "https://vega.github.io/vega-lite/data/cars.json"
  },
  "encoding": {
    "color": {
      "field": "Cylinders",
      "type": "ordinal"
    },
    "shape": {
      "field": "Origin",
      "type": "nominal"
    },
    "x": {
      "field": "Horsepower",
      "type": "quantitative"
    },
    "y": {
      "field": "Miles_per_Gallon",
      "type": "quantitative"
    }
  },
  "height": 200,
  "mark": {
    "size": 100,
    "type": "point"
  },
  "title": "Car Scatter",
  "width": 200
}
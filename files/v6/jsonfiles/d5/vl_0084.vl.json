{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/olih/simulation-experiments/fa14ab812e138880f2509f60e73a7ee38f7ce1cb/exp3/simulation3.csv",
    "format": {
      "type": "csv"
    }
  },
  "title": "Accepted assets from reviewed assets",
  "transform": [],
  "mark": "rect",
  "width": 300,
  "height": 200,
  "encoding": {
    "x": {
      "bin": {
        "maxbins": 100
      },
      "title": "Available hours",
      "field": "available_time_h",
      "type": "quantitative"
    },
    "y": {
      "bin": {
        "maxbins": 100
      },
      "title": "Reviewed assets",
      "field": "reviewed_assets",
      "type": "quantitative"
    },
    "color": {
      "title": "Accepted assets",
      "field": "accepted_assets",
      "type": "quantitative"
    }
  },
  "config": {
    "view": {
      "stroke": "transparent"
    }
  }
}
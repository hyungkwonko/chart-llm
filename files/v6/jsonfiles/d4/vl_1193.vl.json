{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "World Tennis Rankings and Age",
    "subtitle": [
      "Source: ESPN"
    ],
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/Ben25Walker/Ben25Walker.github.io/main/week4tennis.csv"
  },
  "mark": {
    "type": "circle",
    "color": "darkblue"
  },
  "height": 300,
  "width": 300,
  "encoding": {
    "x": {
      "field": "POINTS",
      "type": "quantitative",
      "title": "Points"
    },
    "y": {
      "field": "AGE",
      "type": "quantitative",
      "title": "Age"
    }
  },
  "config": {}
}
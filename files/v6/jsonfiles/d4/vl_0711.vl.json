{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Gini Coefficient, 1980-2021",
    "subtitle": [
      "Source: WID"
    ],
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/maddyht20/maddyht20.github.io/main/WID%20New%201980-2021%20Line%20chart%20CSV.csv"
  },
  "mark": "line",
  "encoding": {
    "x": {
      "field": "Year",
      "type": "temporal"
    },
    "y": {
      "field": "Gini",
      "type": "quantitative"
    },
    "color": {
      "field": "Country",
      "type": "nominal"
    },
    "tooltip": [
      {
        "field": "Country",
        "type": "nominal",
        "title": "Country"
      },
      {
        "field": "Gini",
        "type": "quantitative",
        "title": "Gini"
      }

{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://api.economicsobservatory.com/gbr/unem",
    "format": {
      "type": "json",
      "property": "raw.years"
    }
  },
  "title": {
    "text": "Unemployment level (16+)",
    "subtitle": "United Kingdom (thousands) seasonally adjusted",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "width": 300,
  "height": 300,
  "mark": {
    "type": "line",
    "color": "seagreen",
    "strokeWidth": 2,
    "opacity": 1
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "title": null
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "title": null
    }
  }
}
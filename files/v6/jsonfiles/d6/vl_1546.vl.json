{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/RDeconomist/RDeconomist.github.io/main/data/us/data_US_FRED-UNRATE.json",
    "format": {
      "type": "json",
      "property": "observations"
    }
  },
  "title": {
    "text": "Simple Vega Chart"
  },
  "width": 300,
  "height": 300,
  "mark": {
    "type": "line",
    "color": "seagreen",
    "strokeWidth": 2,
    "opacity": 1
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "title": null
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "title": null
    }
  }
}
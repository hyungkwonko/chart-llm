{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://vega.github.io/vega-lite/data/cars.json"
  },
  "encoding": {
    "color": {
      "field": "Origin",
      "legend": null,
      "type": "nominal"
    },
    "x": {
      "axis": {
        "labelExpr": "datum.label + ' horses'"
      },
      "field": "Horsepower",
      "type": "quantitative"
    },
    "y": {
      "axis": {
        "labelExpr": "datum.label+' mpg'"
      },
      "field": "Miles_per_Gallon",
      "type": "quantitative"
    }
  },
  "mark": "point"
}
{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 600,
  "height": 400,
  "padding": 5,
  "data": {
    "url": "https://raw.githubusercontent.com/segunolalive/daily-gas-prices/049d6eb60825275229ab933cd6251752296e29bb/docs/daily_prices.csv"
  },
  "mark": {
    "type": "line"
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal"
    },
    "y": {
      "field": "price",
      "type": "quantitative"
    },
    "tooltip": [
      {
        "field": "date",
        "type": "temporal"
      },
      {
        "field": "price",
        "type": "quantitative"
      }
    ]
  }
}
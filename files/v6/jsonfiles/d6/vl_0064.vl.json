{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "mark": {
    "type": "bar",
    "tooltip": true
  },
  "encoding": {
    "y": {
      "field": "Salary",
      "type": "quantitative",
      "aggregate": "mean",
      "axis": {
        "format": "s"
      }
    },
    "x": {
      "field": "Country",
      "type": "nominal",
      "aggregate": null,
      "sort": "-y"
    }
  },
  "transform": [],
  "data": {
    "url": "https://raw.githubusercontent.com/nl4dv/nl4dv/master/examples/assets/data/euro.csv",
    "format": {
      "type": "csv"
    }
  }
}
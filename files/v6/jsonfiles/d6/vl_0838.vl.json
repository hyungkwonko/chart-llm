{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/Jordanboi58/Jordanboi58.github.io/main/data_ONS-DJT7.json",
    "format": {
      "type": "json",
      "property": "years"
    }
  },
  "title": {
    "text": "Output per hour: Admin & support services",
    "subtitle": " ",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "width": 200,
  "height": 200,
  "mark": {
    "type": "line",
    "color": "seagreen",
    "strokeWidth": 2,
    "opacity": 1
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "title": "Year"
    },
    "y": {
      "scale": {
        "domain": [
          40,
          130
        ]
      },
      "field": "value",
      "type": "quantitative",
      "title": "Index base, Yr 2019 = 100"
    }
  }
}
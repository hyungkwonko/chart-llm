{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 700,
  "height": 600,
  "data": {
    "url": "https://raw.githubusercontent.com/declann/calculang-miscellaneous-models/080fc8d460470bcac643876d21e354ca8f4438c9/models/hearts/beating-data.json"
  },
  "encoding": {
    "x": {
      "field": "x_in",
      "type": "quantitative",
      "axis": {
        "tickCount": 5
      }
    },
    "y": {
      "axis": {
        "tickCount": 5
      }
    }
  },
  "layer": [
    {
      "mark": {
        "type": "line",
        "tooltip": true,
        "strokeWidth": 5
      },
      "encoding": {
        "y": {
          "field": "trend",
          "type": "quantitative"
        },
        "opacity": {
          "value": 0.08
        },
        "color": {
          "value": "black"
        }
      }
    },
    {
      "mark": {
        "type": "line",
        "tooltip": true
      },
      "encoding": {
        "y": {
          "field": "wavey",
          "type": "quantitative"
        },
        "opacity": {
          "value": 0.07
        },
        "color": {
          "value": "blue"
        }
      }
    },
    {
      "mark": {
        "type": "line",
        "tooltip": true,
        "point": true,
        "interpolate": "monotone",
        "strokeWidth": 0.9
      },
      "encoding": {
        "y": {
          "field": "y",
          "type": "quantitative"
        },
        "color": {
          "value": "red"
        }
      }
    },
    {
      "mark": {
        "type": "line",
        "tooltip": true,
        "strokeWidth": 6
      },
      "encoding": {
        "y": {
          "field": "arcy",
          "type": "quantitative"
        },
        "opacity": {
          "value": 0.08
        },
        "color": {
          "value": "green"
        }
      }
    }
  ]
}
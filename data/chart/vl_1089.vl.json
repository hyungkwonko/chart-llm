{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/Emilysquires02/Emilysquires02.github.io/main/data_ONS-lf24.json",
    "format": {
      "type": "json",
      "property": "years"
    }
  },
  "title": {
    "text": "Employment rate (%) 1971-2021 ",
    "subtitle": "(aged 16 to 64, seasonally adjusted), Source: ONS",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "width": 180,
  "height": 180,
  "mark": {
    "type": "line",
    "color": "seagreen"
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {
        "grid": false
      },
      "title": ""
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": {
        "grid": false
      },
      "title": "Employment rate (%)"
    }
  },
  "config": {}
}
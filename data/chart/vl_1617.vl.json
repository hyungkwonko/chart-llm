{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://vega.github.io/vega-lite/data/cars.json"
  },
  "encoding": {
    "x": {
      "field": "Year",
      "timeUnit": "year",
      "type": "temporal"
    },
    "y": {
      "field": "Miles_per_Gallon",
      "scale": {
        "zero": false
      },
      "title": "Miles per Gallon (1 stdev)",
      "type": "quantitative"
    }
  },
  "mark": {
    "borders": false,
    "extent": "stdev",
    "interpolate": "monotone",
    "type": "errorband"
  }
}